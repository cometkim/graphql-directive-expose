{
  "presets": [
    "@babel/preset-typescript"
  ],
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": 12
          },
          "corejs": 3,
          "useBuiltIns": "usage"
        }]
      ]
    },
    "esmodule": {
      "presets": [
        "@babel/preset-modules"
      ],
      "plugins": [
        ["babel-plugin-module-extension-resolver", {
          "srcExtensions": [".ts"],
          "dstExtension": ".mjs"
        }]
      ]
    }
  }
}
